generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
  schemas         = ["public", "auth"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Clients {
  id         Int      @id @default(autoincrement())
  name       String
  lastname   String
  email      String   @unique
  phone      String
  category   String
  merch      String   @default("Ninguno")
  created_at DateTime @default(now())
}
